{
    "manifest_version": 1,
    "name": "Pomodoro : Tomato boost",
    "version": "1.0",
    "description": "Pomodoro Chrome extension to boost your productivity.",
    "icons": {
        "16": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Pomodoro : Tomato boost"
    },
    "permissions": [
        "activeTab"
    ]
}
